{
  "name": "granam/yaml-reader",
  "license": "MIT",
  "type": "library",
  "description": "Just parse YAML content to array",
  "keywords": [
    "yaml",
    "content",
    "reader"
  ],
  "autoload": {
    "psr-4": {
      "Granam\\YamlReader\\": "YamlReader"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Granam\\Tests\\YamlReader\\": "tests/YamlReader"
    }
  },
  "require": {
    "php": ">=7.3",
    "ext-yaml": "*",
    "granam/strict-object": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0",
    "roave/security-advisories": "dev-latest"
  },
  "config": {
    "sort-packages": true
  }
}
