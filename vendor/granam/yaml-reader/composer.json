{
  "name": "granam/yaml-reader",
  "license": "MIT",
  "type": "library",
  "description": "Just parse YAML content to array",
  "keywords": [
    "yaml",
    "content",
    "reader"
  ],
  "autoload": {
    "psr-4": {
      "Granam\\": "Granam/"
    }
  },
  "require": {
    "php": ">=7.1",
    "ext-yaml": "*",
    "granam/strict-object": "^3.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "~7.0"
  },
  "config": {
    "sort-packages": true
  }
}
