{
  "name": "drdplus/properties",
  "license": "MIT",
  "type": "library",
  "description": "Drd+ properties",
  "autoload": {
    "psr-4": {
      "DrdPlus\\Properties\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DrdPlus\\Tests\\Properties\\": "tests/Properties",
      "DrdPlus\\Tests\\BaseProperties\\": "vendor/drdplus/base-properties/tests/BaseProperties",
      "Granam\\Tests\\ExceptionsHierarchy\\": "vendor/granam/exception-hierarchy/tests/ExceptionsHierarchy"
    }
  },
  "require": {
    "php": ">=7.4",
    "granam/integer-enum": "^1.1",
    "granam/float-enum": "^1.0",
    "granam/boolean-enum": "^1.0",
    "drdplus/tables": "^3.0",
    "drdplus/calculations": "^1.0",
    "drdplus/base-properties": "^3.0",
    "granam/integer": "^7.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^9.3",
    "mockery/mockery": "^1.2",
    "granam/exceptions-hierarchy": "^5.0",
    "granam/test-with-mockery": "^2.0",
    "granam/exception-hierarchy": "^5.1",
    "rector/rector": "^0.11.53"
  }
