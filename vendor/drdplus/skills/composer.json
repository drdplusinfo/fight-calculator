{
  "name": "drdplus/skills",
  "license": "MIT",
  "type": "library",
  "description": "Skills for Drd+",
  "autoload": {
    "psr-4": {
      "DrdPlus\\Skills\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DrdPlus\\Tests\\": "tests",
      "Granam\\Tests\\ExceptionsHierarchy\\": "vendor/granam/exceptions-hierarchy/tests/ExceptionsHierarchy"
    }
  },
  "require": {
    "php": ">=7.4",
    "drdplus/background": "~3.0",
    "drdplus/lighting": "~3.0",
    "drdplus/hunting-and-fishing": "~3.0",
    "drdplus/profession-levels": "~3.0",
    "drdplus/armourer": "^3.0"
  },
  "require-dev": {
    "granam/exceptions-hierarchy": "~5.0",
    "granam/test-with-mockery": "^2.0",
    "mockery/mockery": "~1.2",
    "phpunit/phpunit": "~9.0",
    "rector/rector": "^0.11.53",
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "sort-packages": true
  }
}
