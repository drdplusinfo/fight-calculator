{
  "name": "drdplus/background",
  "license": "MIT",
  "type": "library",
  "description": "Background of a person for Drd+",
  "autoload": {
    "psr-4": {
      "DrdPlus\\Background\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Granam\\Tests\\ExceptionsHierarchy\\": "vendor/granam/exceptions-hierarchy/tests/ExceptionsHierarchy",
      "DrdPlus\\Tests\\": "tests"
    }
  },
  "require": {
    "php": ">=7.4",
    "drdplus/professions": "~3.0"
  },
  "require-dev": {
    "granam/exceptions-hierarchy": "^5.0",
    "granam/test-with-mockery": "^2.0",
    "mockery/mockery": "^1.2",
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "dev-latest"
  },
  "config": {
    "sort-packages": true
  }
}
