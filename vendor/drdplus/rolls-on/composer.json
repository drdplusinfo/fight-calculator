{
  "name": "drdplus/rolls-on",
  "license": "MIT",
  "type": "library",
  "description": "Drd+ rolls by dices covering specific situations",
  "autoload": {
    "psr-4": {
      "DrdPlus\\RollsOn\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DrdPlus\\Tests\\": "tests",
      "Granam\\Tests\\ExceptionsHierarchy\\": "vendor/granam/exceptions-hierarchy/tests/ExceptionsHierarchy"
    }
  },
  "require": {
    "php": ">=7.4",
    "drdplus/properties": "^3.0"
  },
  "require-dev": {
    "granam/exceptions-hierarchy": "^5.0",
    "granam/test-with-mockery": "^2.0",
    "mockery/mockery": "^1.2",
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "dev-latest"
  },
  "config": {
    "sort-packages": true
  }
}
