{
  "name": "drdplus/calculator-skeleton",
  "license": "MIT",
  "type": "composer-plugin",
  "description": "Style and functionality helper of a DrD+ calculator",
  "autoload": {
    "psr-4": {
      "DrdPlus\\": "DrdPlus/"
    }
  },
  "require": {
    "php": ">=7.1",
    "composer-plugin-api": "^1.1",
    "drdplus/rules-skeleton": "^1.5",
    "granam/integer": "^7.0"
  },
  "require-dev": {
    "composer/composer": "^1.9",
    "granam/exceptions-hierarchy": "^5.0",
    "mockery/mockery": "^1.2",
    "phpunit/phpunit": "~8.3",
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "class": "DrdPlus\\CalculatorSkeleton\\CalculatorInjectorComposerPlugin"
  }
}
