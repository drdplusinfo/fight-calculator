{
  "name": "drdplus/calculator-skeleton",
  "license": "MIT",
  "type": "composer-plugin",
  "description": "Style and functionality helper of a DrD+ calculator",
  "autoload": {
    "psr-4": {
      "DrdPlus\\CalculatorSkeleton\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\DrdPlus\\CalculatorSkeleton\\": "tests/CalculatorSkeleton",
      "Tests\\DrdPlus\\RulesSkeleton\\": "vendor/drdplus/rules-skeleton/tests/RulesSkeleton",
      "Granam\\Tests\\ExceptionsHierarchy\\": "vendor/granam/exceptions-hierarchy/tests/ExceptionsHierarchy"
    }
  },
  "require": {
    "php": ">=7.4",
    "composer-plugin-api": "^1.1|^2.0",
    "composer/composer": "^2.1",
    "drdplus/rules-skeleton": "^3.2",
    "granam/git": "^2.1",
    "granam/integer": "^7.0",
    "granam/strict-object": "^3.1",
    "granam/string": "^4.1",
    "granam/web-content-builder": "^2.3",
    "matomo/device-detector": "^4.3"
  },
  "require-dev": {
    "granam/exceptions-hierarchy": "^5.1",
    "granam/test-with-mockery": "^2.0",
    "mockery/mockery": "^1.2",
    "phpunit/phpunit": "~9.0",
    "roave/security-advisories": "dev-latest",
    "ext-json": "*"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "class": "DrdPlus\\CalculatorSkeleton\\CalculatorInjectorComposerPlugin"
  }
}
